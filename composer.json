{
    "name": "jessegall/lightspeed-sdk",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JesseGall\\LightspeedSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JesseGall\\LightspeedSDK\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.dev.php"
        ]
    },
    "authors": [
        {
            "name": "Jesse Gall",
            "email": "jesse-gall@hotmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "seoshop/seoshop-php": "~1.9",
        "vlucas/phpdotenv": "^5.4",
        "jessegall/proxy": "dev-main",
        "jessegall/invader": "^1.0",
        "jessegall/resources": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1",
        "spatie/ray": "^1.36"
    },
    "scripts": {
        "test": "@php -dxdebug.mode=coverage ./vendor/bin/phpunit --testdox --coverage-html=report --colors=auto",
        "test-filter": "@test --filter="
    }
}
